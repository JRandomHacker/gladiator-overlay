{
  "name": "gladiator-overlay",
  "version": "0.0.0",
  "description": "Graphics for Gladiator Tournament Coverage",
  "homepage": "https://twitch.tv/gladiatormtga",
  "author": {
    "name": "Justin Koehler",
    "email": "jrandomhacker172342@gmail.com",
    "url": ""
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js"
  ],
  "keywords": [
    "magic",
    "mtga",
    "gladiator",
    "nodecg-bundle"
  ],
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "build-verbose": "parcel build --log-level verbose",
    "watch": "parcel watch"
  },
  "targets": {
    "graphic": {
      "source": "src/index.html",
      "distDir": "graphics",
      "publicUrl": "./"
    },
    "panel": {
      "source": "src/panel.html",
      "distDir": "dashboard",
      "publicUrl": "./"
    },
    "extension": {
      "source": "src/extension.js",
      "main": "extension.js",
      "publicUrl": "./"
    }
  },
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "panel",
        "title": "Controls",
        "width": 2,
        "file": "panel.html",
        "headerColor": "#525F78"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.0.1"
  },
  "dependencies": {
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-input": "^3.2.1",
    "csv": "^6.0.5",
    "node-fetch-cookies": "^2.0.3"
  }
}
